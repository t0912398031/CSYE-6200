	}

	public static void main(String[] args) {
		 List<Person> people = new ArrayList<>();
		 
		 people.add(
		       new Person.Builder()
			            .givenName("Bob")
			            .surName("Baker")
			           .age(21)
			           .gender("MALE")
			            .email("bob.baker@example.com")
