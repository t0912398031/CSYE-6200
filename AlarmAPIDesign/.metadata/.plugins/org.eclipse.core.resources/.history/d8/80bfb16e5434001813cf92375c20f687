package edu.neu.csye6200;

import java.time.LocalDate;

public abstract class AbstractAlarmAPI {

	public abstract void alarm();
	public abstract String alarmInfo(LocalDate date);
	public abstract String timeRemaining();
	public abstract void overDuedAlarm();
	public abstract void emailReminder();
	public abstract void SMSReminder();//Short Message Service
	public abstract void mailReminder();
	
	
}
